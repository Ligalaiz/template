          { how: 'How It Work With Us' },
        ],
    },
    {
      id: 'follow',
      title: 'Follow US',
      submenu: [
        { inst:'Instagram' },
        { fb: 'Facebook' },
        { ln: 'LinkedIn' },
        { youtube: 'Youtube' }
      ],
    },
  ];

mixin Header
    header.header#header
      .header__wrap
        a(href="/")
          .header__logo#logo
              +image('logo')
        +MenuBtn
        +Navigation(navItems)
